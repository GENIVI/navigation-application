package org.automotive

interface Manager {
	version {
		major 1 
		minor 0
	}
	
	method FindObject {
		in {
			String 	searchstring
		}
		out {
			String [] response
		}	
	}

	method FindObjectForZone {
		in {
			String 	searchstring
			Int32	zone
		}
		out {
			String response
		}	
	}

	method FindObjectForSourceZone {
		in {
			String 	searchstring
			String	source
			Int32	zone
		}
		out {
			String response
		}	
	}

	method List {
		out {
			String [] response	
		}
	}
	
	method ZonesForObjectName {
		in {
			String searchstring
		}
		out {
			Int32 [] response
		}
	}

	method SourceForObjectName {
		in {
			String searchstring
		}
		out {
			String [] response
		}
	}
	
}